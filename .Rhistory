OfficeBasedVisitsClean$MEDPRESC[OfficeBasedVisitsClean$MEDPRESC == 1] <- 'Yes'
OfficeBasedVisitsClean$MEDPRESC[OfficeBasedVisitsClean$MEDPRESC == 2] <- 'No'
OfficeBasedVisitsClean
